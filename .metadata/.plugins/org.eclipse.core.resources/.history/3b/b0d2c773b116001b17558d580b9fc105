package Chapter5;

public class CoffePerson {
	String name;
	int money;
	
	public CoffePerson(String name, int money) {
		this.name = name;
		this.money = money;
	}
	public void buySarCoffee(StarCoffee cafe, Menu Menu.STARAMERICANO) {
		
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getMoney() {
		return money;
	}
	public void setMoney(int money) {
		this.money = money;
	}
	
	
}
