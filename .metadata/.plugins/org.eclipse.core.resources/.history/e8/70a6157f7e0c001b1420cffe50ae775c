package reference;

public class StudentRef {
	String studentName;
	int studentAge;
	SubjectRef koreaSubject;
	SubjectRef mathSubject;
	
	
	public StudentRef(String name, int age) {
		studentName = name;
		studentAge = age;
		
		koreaSubject = new SubjectRef();
		mathSubject = new SubjectRef();
	}
	
	public void setKoreaSubject(int score) {
		koreaSubject = score;
		
	
	}
	
	public void setKoreaSubject(int score) {
		mathSubject = score;
		
	
	}
	
	
	
	
	
}
